{
  "version": 2,
  "framework": null,
  "public": true,
  "buildCommand": "echo Building",
  "routes": [
    {
      "src": "/api/scans/bulk",
      "dest": "/api/scans-bulk.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/scans/count",
      "dest": "/api/scans-count.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/(.*\\..*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ]
}
